# HabitGraph (based on reference/archsoft/database/.env.example)

# Common
DB_HOST=localhost

# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=habitgraph
POSTGRES_PASSWORD=habitgraph
POSTGRES_DB=habitgraph

# MongoDB
MONGO_HOST=localhost
MONGO_PORT=27017
MONGO_USER=root
MONGO_PASSWORD=secret
MONGO_DB=habitgraph

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Qdrant
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=habitgraph_diary_entries

# Neo4j (Bolt)
NEO4J_HOST=localhost
NEO4J_PORT=7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=habitgraph

# RabbitMQ (optional; used for event publishing)
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest

